{% set pageTitle = "Html" %}
{% extends "html/_includes/main" %}

{% block content %}
<section class="my-16 md:my-28 mt-40 lg:mt-40">
    <div class="container">
        <div class="grid grid-cols-1">
            <div class="overflow-x-auto">
                <div class="inline-block min-w-full">
                    <div class="overflow-hidden">
                        <table class="min-w-full whitespace-nowrap border border-gray-50 rounded-lg overflow-hidden">
                            <thead class="bg-gray-100/50 text-crimson-500 border-b">
                                <tr>
                                    <th scope="col" class="font-medium px-6 py-4">
                                        Page Name
                                    </th>
                                    <th scope="col" class="font-medium px-6 py-4">
                                        Page Link
                                    </th>
                                </tr>
                            </thead>
                            <tbody>
                                <tr class="bg-white border-b border-gray-100 last:border-0">
                                    <td class="px-6 py-4 font-medium whitespace-nowrap">Home Page</td>
                                    <td class="px-6 py-4 font-medium whitespace-nowrap text-neutral-500 hover:text-cyprus-500 transition-all duration-300"><a href="/html/home">Home Page</a></td>
                                </tr>
                                <tr class="bg-white border-b border-gray-100 last:border-0">
                                    <td class="px-6 py-4 font-medium whitespace-nowrap">Work</td>
                                    <td class="px-6 py-4 font-medium whitespace-nowrap text-neutral-500 hover:text-cyprus-500 transition-all duration-300"><a href="/html/work">Work</a></td>
                                </tr>
                                <tr class="bg-white border-b border-gray-100 last:border-0">
                                    <td class="px-6 py-4 font-medium whitespace-nowrap">Work Detail</td>
                                    <td class="px-6 py-4 font-medium whitespace-nowrap text-neutral-500 hover:text-cyprus-500 transition-all duration-300"><a href="/html/work-detail">Work Detail</a></td>
                                </tr>
                                <tr class="bg-white border-b border-gray-100 last:border-0">
                                    <td class="px-6 py-4 font-medium whitespace-nowrap">About</td>
                                    <td class="px-6 py-4 font-medium whitespace-nowrap text-neutral-500 hover:text-cyprus-500 transition-all duration-300"><a href="/html/about">About</a></td>
                                </tr>
                                <tr class="bg-white border-b border-gray-100 last:border-0">
                                    <td class="px-6 py-4 font-medium whitespace-nowrap">Process</td>
                                    <td class="px-6 py-4 font-medium whitespace-nowrap text-neutral-500 hover:text-cyprus-500 transition-all duration-300"><a href="/html/process">Process</a></td>
                                </tr>
                                <tr class="bg-white border-b border-gray-100 last:border-0">
                                    <td class="px-6 py-4 font-medium whitespace-nowrap">Blog</td>
                                    <td class="px-6 py-4 font-medium whitespace-nowrap text-neutral-500 hover:text-cyprus-500 transition-all duration-300"><a href="/html/blog">Blog</a></td>
                                </tr>
                                <tr class="bg-white border-b border-gray-100 last:border-0">
                                    <td class="px-6 py-4 font-medium whitespace-nowrap">Blog Detail</td>
                                    <td class="px-6 py-4 font-medium whitespace-nowrap text-neutral-500 hover:text-cyprus-500 transition-all duration-300"><a href="/html/blog-detail">Blog Detail</a></td>
                                </tr>
                                <tr class="bg-white border-b border-gray-100 last:border-0">
                                    <td class="px-6 py-4 font-medium whitespace-nowrap">Contact</td>
                                    <td class="px-6 py-4 font-medium whitespace-nowrap text-neutral-500 hover:text-cyprus-500 transition-all duration-300"><a href="/html/contact">Contact</a></td>
                                </tr>
                            </tbody>
                        </table>
                    </div>
                </div>
            </div>
        </div>
    </div>
</section>

<div>
    <div class="text-cyprus-50"></div>
    <div class="text-cyprus-100"></div>
    <div class="text-cyprus-200"></div>
    <div class="text-cyprus-300"></div>
    <div class="text-cyprus-400"></div>
    <div class="text-cyprus-500"></div>
    <div class="text-cyprus-600"></div>
    <div class="text-cyprus-700"></div>
    <div class="text-cyprus-800"></div>
    <div class="text-cyprus-900"></div>
    <div class="text-cyprus-950"></div>
</div>
{% endblock %}
